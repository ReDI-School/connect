{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "redi-connect",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "redi-connect": {
      "root": "apps/redi-connect",
      "sourceRoot": "apps/redi-connect/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/redi-connect",
            "index": "apps/redi-connect/src/index.html",
            "main": "apps/redi-connect/src/main.tsx",
            "polyfills": "apps/redi-connect/src/polyfills.ts",
            "tsConfig": "apps/redi-connect/tsconfig.app.json",
            "assets": ["apps/redi-connect/src/assets"],
            "styles": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/shared-atomic-design-components/src/lib/styles"
              ]
            },
            "extractCss": true,
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/redi-connect/src/environments/environment.ts",
                  "with": "apps/redi-connect/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "redi-connect:build",
            "port": 3000,
            "watch": false
          },
          "configurations": {
            "production": { "buildTarget": "redi-connect:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/redi-connect/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/redi-connect"],
          "options": {
            "jestConfig": "apps/redi-connect/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redi-connect-e2e": {
      "root": "apps/redi-connect-e2e",
      "sourceRoot": "apps/redi-connect-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/redi-connect-e2e/cypress.json",
            "tsConfig": "apps/redi-connect-e2e/tsconfig.e2e.json",
            "devServerTarget": "redi-connect:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "redi-connect:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/redi-connect-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "admin-panel": {
      "root": "apps/admin-panel",
      "sourceRoot": "apps/admin-panel/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/admin-panel",
            "index": "apps/admin-panel/src/index.html",
            "main": "apps/admin-panel/src/main.tsx",
            "polyfills": "apps/admin-panel/src/polyfills.ts",
            "tsConfig": "apps/admin-panel/tsconfig.app.json",
            "assets": ["apps/admin-panel/src/assets"],
            "styles": ["apps/admin-panel/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin-panel/src/environments/environment.ts",
                  "with": "apps/admin-panel/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "admin-panel:build", "port": 3001 },
          "configurations": {
            "production": { "buildTarget": "admin-panel:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/admin-panel/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/admin-panel"],
          "options": {
            "jestConfig": "apps/admin-panel/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-panel-e2e": {
      "root": "apps/admin-panel-e2e",
      "sourceRoot": "apps/admin-panel-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-panel-e2e/cypress.json",
            "tsConfig": "apps/admin-panel-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin-panel:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "admin-panel:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/admin-panel-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              { "command": "node apps/api/server/server.js" },
              { "command": "mongod --dbpath ./mongodb-data" }
            ]
          }
        }
      }
    },
    "shared-atomic-design-components": {
      "root": "libs/shared-atomic-design-components",
      "sourceRoot": "libs/shared-atomic-design-components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-atomic-design-components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-atomic-design-components"],
          "options": {
            "jestConfig": "libs/shared-atomic-design-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-types": {
      "root": "libs/shared-types",
      "sourceRoot": "libs/shared-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/shared-types/**/*.ts"] }
        }
      }
    }
  }
}
