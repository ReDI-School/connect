{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "node apps/api/server/server.js"
              },
              {
                "command": "mongod --dbpath ./mongodb-data"
              }
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": []
    },
    "common-types": {
      "root": "libs/common-types",
      "sourceRoot": "libs/common-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/common-types/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common-types"],
          "options": {
            "jestConfig": "libs/common-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
